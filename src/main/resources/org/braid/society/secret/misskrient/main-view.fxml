<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<BorderPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/19"
  fx:controller="org.braid.society.secret.misskrient.controller.MisskrientController">
  <center>
    <BorderPane BorderPane.alignment="CENTER" prefHeight="200.0" prefWidth="200.0"
      styleClass="timelineElementIconPane">
      <center>
        <HBox styleClass="timelineColumnPane">
               <children>
                 <VBox prefHeight="200.0" prefWidth="100.0" styleClass="timelineColumn"
                   fx:id="timelineColumn"/>
               </children>
            </HBox>
      </center>
      <left>
        <Button BorderPane.alignment="CENTER" mnemonicParsing="false"
          onAction="#onColumnSwitchButtonPressed" styleClass="commonButton" text="&lt;"/>
      </left>
      <right>
        <Button BorderPane.alignment="CENTER" mnemonicParsing="false"
          onAction="#onColumnSwitchButtonPressed" styleClass="commonButton" text="&gt;"/>
      </right>
      <top>
        <HBox BorderPane.alignment="CENTER" prefHeight="100.0" prefWidth="200.0"
          styleClass="commonInformationBar"/>
      </top>
    </BorderPane>
   </center>
  <left>
    <VBox BorderPane.alignment="CENTER" styleClass="funcBox">
      <children>
        <Button mnemonicParsing="false" onAction="#onFuncButtonPressed" text="Button"
          fx:id="funcButton">
          <styleClass>
            <String fx:value="commonButton"/>
            <String fx:value="funcButton"/>
          </styleClass>
        </Button>
      </children>
    </VBox>
  </left>
</BorderPane>
